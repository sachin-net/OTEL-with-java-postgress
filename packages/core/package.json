{
  "name": "@swaggerstats/core",
  "packageManager": "yarn@3.1.1",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "swagger-stats core package",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "compile": "yarn run -T tsc --build",
    "postcompile": "cpy **/* !**/*.ts ../build/src --cwd=./src --no-overwrite --parents",
    "list": "yarn run -T tsc --listFiles",
    "clean": "tsc --build --clean",
    "test": "jest --verbose",
    "test-mem": "node --expose-gc ../../node_modules/jest/bin/jest.js --runInBand --logHeapUsage",
    "test2": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "yarn run -T eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch": "tsc --build --watch"
  },
  "keywords": [
    "opentracing",
    "tracing",
    "api",
    "observability",
    "telemetry",
    "node",
    "express",
    "koa",
    "hapi",
    "fastify",
    "rest",
    "statistics",
    "monitoring",
    "alerting",
    "swagger",
    "openapi",
    "schema",
    "prometheus",
    "metrics",
    "elasticsearch",
    "kibana",
    "grafana"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slanatech/swagger-stats"
  },
  "author": "https://github.com/sv2",
  "homepage": "http://swaggerstats.io",
  "bugs": {
    "url": "https://github.com/slanatech/swagger-stats/issues",
    "email": "sv2@slana.tech"
  },
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.0.3",
    "@opentelemetry/auto-instrumentations-node": "^0.27.0",
    "@opentelemetry/core": "^1.0.1",
    "@opentelemetry/exporter-jaeger": "^1.0.1",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.27.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.27.0",
    "@opentelemetry/instrumentation": "^0.27.0",
    "@opentelemetry/instrumentation-http": "^0.26.0",
    "@opentelemetry/instrumentation-ioredis": "^0.25.0",
    "@opentelemetry/instrumentation-pg": "^0.25.0",
    "@opentelemetry/resources": "^1.0.1",
    "@opentelemetry/sdk-trace-base": "^1.0.1",
    "@opentelemetry/sdk-trace-node": "^1.0.1",
    "@opentelemetry/semantic-conventions": "^1.0.1",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.60",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "cpy-cli": "^3.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0"
  }
}
