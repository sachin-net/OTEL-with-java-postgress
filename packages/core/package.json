{
  "name": "@swaggerstats/core",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "swagger-stats core package",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "compile": "tsc --build",
    "postcompile": "cpy **/* !**/*.ts ../build/src --cwd=./src --no-overwrite --parents",
    "list": "tsc --listFiles",
    "clean": "tsc --build --clean",
    "test": "jest --verbose",
    "test-mem": "node --expose-gc ../../node_modules/jest/bin/jest.js --runInBand --logHeapUsage",
    "test2": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch": "tsc --build --watch"
  },
  "keywords": [
    "opentracing",
    "tracing",
    "api",
    "observability",
    "telemetry",
    "node",
    "express",
    "koa",
    "hapi",
    "fastify",
    "rest",
    "statistics",
    "monitoring",
    "alerting",
    "swagger",
    "openapi",
    "schema",
    "prometheus",
    "metrics",
    "elasticsearch",
    "kibana",
    "grafana"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slanatech/swagger-stats"
  },
  "author": "https://github.com/sv2",
  "homepage": "http://swaggerstats.io",
  "bugs": {
    "url": "https://github.com/slanatech/swagger-stats/issues",
    "email": "sv2@slana.tech"
  },
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.0.2",
    "@opentelemetry/core": "^0.24.0",
    "@opentelemetry/node": "^0.24.0",
    "@opentelemetry/instrumentation": "^0.24.0",
    "@opentelemetry/instrumentation-http": "^0.24.0",
    "@opentelemetry/instrumentation-express": "^0.24.0",
    "@opentelemetry/instrumentation-hapi": "^0.24.0",
    "@opentelemetry/tracing": "^0.24.0",
    "@opentelemetry/exporter-prometheus": "^0.24.0",
    "@opentelemetry/exporter-jaeger": "^0.24.0",
    "@opentelemetry/exporter-collector-grpc": "^0.24.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
  },
  "devDependencies": {
    "cpy-cli": "^3.1.1"
  }
}
