{
  "name": "@swaggerstats/node",
  "version": "1.0.0-alpha.1",
  "private": true,
  "description": "swagger-stats node embedded observability",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "compile": "tsc --build",
    "postcompile": "ncp ../../node_modules/@swaggerstats/ui/dist ./build/ui",
    "clean": "tsc --build --clean",
    "test": "jest",
    "test2": "nyc ts-mocha -p tsconfig.json 'test/**/*.test.ts'",
    "tdd": "npm run test -- --watch-extensions ts --watch",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json -p ../../",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "watch": "tsc --build --watch"
  },
  "keywords": [
    "opentracing",
    "tracing",
    "api",
    "observability",
    "telemetry",
    "node",
    "express",
    "koa",
    "hapi",
    "fastify",
    "rest",
    "statistics",
    "monitoring",
    "alerting",
    "swagger",
    "openapi",
    "schema",
    "prometheus",
    "metrics",
    "elasticsearch",
    "kibana",
    "grafana"
  ],
  "files": [
    "src/*",
    "schema/*",
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slanatech/swagger-stats"
  },
  "author": "https://github.com/sv2",
  "homepage": "http://swaggerstats.io",
  "bugs": {
    "url": "https://github.com/slanatech/swagger-stats/issues",
    "email": "sv2@slana.tech"
  },
  "license": "MIT",
  "dependencies": {
    "@swaggerstats/core": "1.0.0-alpha.1",
    "basic-auth": "^2.0.1",
    "cookies": "^0.8.0",
    "debug": "^4.3.1",
    "moment": "^2.29.1",
    "node-static": "^0.7.11",
    "path-to-regexp": "^6.2.0",
    "qs": "^6.10.1",
    "request": "^2.88.2",
    "send": "^0.17.1",
    "uuid": "^8.3.2",
    "ws": "^8.1.0"
  },
  "peerDependencies": {
    "prom-client": ">= 10 <= 13"
  },
  "devDependencies": {
    "@swaggerstats/ui": "1.0.0-alpha.1",
    "@types/debug": "^4.1.7",
    "@types/node-static": "^0.7.7",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "chokidar": "^3.5.1",
    "concurrently": "^6.0.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "istanbul": "^0.4.5",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "prom-client": "^13.1.0",
    "swagger-parser": "^10.0.2",
    "typescript": "^4.3.5"
  }
}
